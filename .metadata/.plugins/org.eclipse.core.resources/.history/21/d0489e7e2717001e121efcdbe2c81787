package JavaProgram;

public class ReverseInterger {

	public void revInt() {
		
		int num = 5;
		
		int fact=1;
		
		for(int i =1; i<=5; i++) {
			
			fact = fact*i;
			
			
			
		}
		System.out.println(fact);
		
		
	
		 
		 
	}
		
		
		
		
	
	
	

	public static void main(String[] args) {
		
		ReverseInterger rt = new ReverseInterger();
		
		rt.revInt();
		
		
		

	}

}
