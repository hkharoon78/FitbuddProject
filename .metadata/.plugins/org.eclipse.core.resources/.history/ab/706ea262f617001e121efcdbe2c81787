package JavaProgram;

public class RevInt {
	int rev=0;
	
	public static int revIntegerNum(int num) {
		return num;
		
		
		//int num = 12345;
		while(num!=0) {
			
			rev = rev*10+num%10;
			num = num/10;
			
			
			
		}
		
		
		
		
		
		
		
	}

	public static void main(String[] args) {
		
		
		revIntegerNum(12345);
		
		

	}

}
