package JavaProgram;

public class ReverseInterger {

	public void revInt() {
		
		int result = 0;
		
	
		
		
		for(int i =1; i<=10; i++) {
			
			result = result+i;
			
			
		}
		

		 
		 
	}
		
		
		
		
	
	
	

	public static void main(String[] args) {
		
		ReverseInterger rt = new ReverseInterger();
		
		rt.revInt();
		
		
		

	}

}
