package JavaProgram;

public class ReverseInterger {
	
	int num = 123456;
	int rev = 0;
	public void revInt() {
		
		while(num!=0) {
			
			num = num*10+num%10;
			rev = rev/10;
			
			
		}
		
		System.out.println(num);
		
		
		
		
	}
	
	

	public static void main(String[] args) {
		
		ReverseInterger rt = new ReverseInterger();
		
		rt.revInt();
		
		
		

	}

}
