package JavaProgram;

public class GivenNumberMonthDay {

	public static void main(String[] args) {
		
		int month = 5;
		
		if(month==1) {
			System.out.println("31");
			
			
		}
		if(month==2) {
			System.out.println("28/29");
			
			
		}
		if(month==3) {
			System.out.println("31");
			
			
		}
		if(month==4) {
			System.out.println("30");
			
			
		}
		if(month==5) {
			System.out.println("31");
			
			
		}
		if(month==6) {
			System.out.println("30");
			
			
		}
		else {
			
			
			System.out.println("Invalid month");
		}
		
		

	}

}
